{
  "LABEL": {
    "name": "title-check",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "prefixes": ["feat:", "fix:", "docs:", "style:", "refactor:", "perf:", "test:", "build:", "ci:", "chore:", "revert:", "content:"],
    "regexp": "^(feat|fix|docs|style|refactor|perf|test|build|ci|chore|revert|content)(\\([\\w\\-\\.]+\\))?:\\s.*",
    "regexpFlags": "i",
    "ignoreLabels": ["ignore-title-check"]
  },
  "MESSAGES": {
    "success": "PR 标题格式正确！",
    "failure": "PR 标题必须以下列前缀之一开头：feat:, fix:, docs:, style:, refactor:, perf:, test:, build:, ci:, chore:, revert:, content:",
    "notice": ""
  }
}
